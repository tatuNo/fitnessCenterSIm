package osat;

import static org.junit.jupiter.api.Assertions.*;

import org.junit.jupiter.api.Test;

/**
 * Testiluokka asiakkaan tyypille.
 * @author Tatu Nordström
 *
 */
class AsiakkaanTyyppiTest {
	

	/**
	 * Testaa ArvoTyyppi metodin, konsolisyötteestä pitää tarkistaa vaihtuvuus.
	 */
	@Test
	void testArvoTyyppi() {
		for (int i = 0; i < 10; i++) {
			System.out.println(AsiakkaanTyyppi.arvoTyyppi());
		}
		System.out.println("----------------------------------");
		fail("Katso konsolisyötteestä että tyypissä on vaihtuvuutta");
	}

	/**
	 * Testaa ArvoSukupuoli metodin, konsolisyötteestä pitää tarkistaa vaihtuvuus.
	 */
	@Test
	void testArvoSukupuoli() {
		for (int i = 0; i < 10; i++) {
			System.out.println(AsiakkaanTyyppi.arvoSukupuoli());
		}
		fail("Katso konsolisyötteestä että sukupuolessa on vaihtuvuutta");
	}

}
